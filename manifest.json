{
    "manifest_version": 2,
    "name": "Builds Tab",
    "version": "0.1",
    "description": "Adds a builds tab to Github repos.",
    "content_scripts": [
        {
            "matches": [
                "*://github.com/*"
            ],
            "js": [
                "vendor/axios.min.js",
                "vendor/cash.min.js",
                "vendor/gh-injection.js",
                "vendor/moment.min.js",
                "vendor/riot.csp.js",
                "dist/components.js",
                "src/adapters/travis.js",
                "src/build.js",
                "src/index.js"
            ]
        }
    ]
}
